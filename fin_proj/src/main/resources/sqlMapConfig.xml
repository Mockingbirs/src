<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
 
 
  <configuration>
  <!-- Alias 설정 -->
  <typeAliases>
  <typeAlias alias="member" type="com.spring.proj.domain.MemberVO"/>
  <typeAlias alias="usermember" type="com.spring.proj.domain.UserMemberVO"/>
  </typeAliases>
  
  <!-- Sql Mapper의 위치 설정 -->
  <mappers>
  <mapper resource="mappings/memberMapping.xml" />
  <mapper resource="mappings/usermemberMapping.xml"/>
  
  </mappers>
  
  
  
  </configuration>
  